# Validar si el tipo de dato Código es válido

def es-numerico(cadena):
	try:
		int(cadena)
		retunr True
	except ValueError:
		return False

numero = input('Escriba dos números para el código del Vivero: ')

if es_numerico(numero):
	print('Ha digitado un valor válido.')
else:
	print('Los valores digitados no corresponden al código.')